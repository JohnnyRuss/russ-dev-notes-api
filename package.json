{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "compile": "tsc -p .",
    "start": "node ./build/server.js",
    "watch": "nodemon --exec ts-node --files ./src/server.ts",
    "copy-views": "copyfiles -u 1 ./src/views/**/* ./build",
    "copy-public": "copyfiles -u 1 ./src/public/**/* ./build",
    "build": "npm run compile && npm run copy-views && npm run copy-public",
    "start:dev": "concurrently \"npm:compile\" \"npm:watch\""
  },
